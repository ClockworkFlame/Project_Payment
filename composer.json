{
    "name": "paymentfee/commission-task-skeleton",
    "description": "Skeleton for paymentfee recruitment task",
    "keywords": [],
    "type": "project",
    "autoload": {
        "files": [
            "src/helper/General.php"
        ],
        "psr-4": {
            "Src\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "scripts": {
        "phpunit": "phpunit",
        "fix-cs": "php-cs-fixer fix",
        "test-cs": "php-cs-fixer fix --dry-run -v",
        "test": ["@phpunit", "@test-cs"]
    }
}
